### BEGIN PacerCMS ###
RewriteEngine On

##### Articles & Archives #####
RewriteRule ^([0-9]+).htm([0-9]+)?/?$ article.php?id=$1&page=$2 [QSA]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+).htm?/?$ archives.php?issue=$1-$2-$3 [QSA]

##### RSS Feeds #####
RewriteRule ^rss\.xml$ feed.php?id=1
RewriteRule ^viewpoints/rss\.xml$ feed.php?id=2
RewriteRule ^opinions/rss\.xml$ feed.php?id=2
RewriteRule ^news/rss\.xml$ feed.php?id=4
RewriteRule ^entertainment/rss\.xml$ feed.php?id=4
RewriteRule ^features/rss\.xml$ feed.php?id=4
RewriteRule ^sports/rss\.xml$ feed.php?id=5

##### Sections #####
RewriteRule ^viewpoints* section.php?id=2
RewriteRule ^news* section.php?id=3
RewriteRule ^entertainment* section.php?id=4
RewriteRule ^sports* section.php?id=5

##### Pages #####
RewriteRule ^about/feedback* feedback.php
RewriteRule ^about* page.php?id=1
RewriteRule ^advertise* page.php?id=2

##### Submitted #####
RewriteRule ^write/feedback* feedback.php
RewriteRule ^write/letters* submit.php?mode=letter
RewriteRule ^write/comments* submit.php?mode=comments
RewriteRule ^write/events* submit.php?mode=bboard
RewriteRule ^write/article* submit.php?mode=article
RewriteRule ^write* submit.php?mode=article

#### Change of Address ####
RewriteRule ^features.* http://pacer.utm.edu/entertainment/ [R=301,L]
RewriteRule ^opinions.* http://pacer.utm.edu/viewpoints/ [R=301,L]
RewriteRule ^firstflight.* http://spj.utm.edu/firstflight/ [R=301,L]
RewriteRule ^jobs.* http://stupubs.utm.edu/ [R=301,L]

#### Force Correct Domain #####
RewriteCond %{HTTP_HOST} ^(www\.)?utm\.edu [NC]
RewriteRule ^(.*)$ http://pacer.utm.edu/$1 [R=301,L]

#### Off-Site Redirects ####
RewriteRule ^weather* http://www.weather.com/weather/local/38238/
RewriteRule ^calendar* http://www.utm.edu/cgi-bin/calcium/Calcium37.pl?CalendarName=univrel
RewriteRule ^alumni* http://www.utmforever.com/
RewriteRule ^skyhawks* http://www.utmsports.com/
RewriteRule ^email* http://xmail.utm.edu/

